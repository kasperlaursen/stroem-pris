{
	"name": "stroem-pris",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:fix": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"supabase:types": "npx supabase gen types typescript --project-id 'sdqtbpqrecvpngdqditz' --schema public > src/lib/data/supabase/types.ts"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.16.1",
		"@playwright/test": "^1.34.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.18.0",
		"@types/luxon": "^3.3.0",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"@vitest/coverage-c8": "^0.31.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.16.1",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"supabase": "^1.62.4",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.31.1"
	},
	"type": "module",
	"dependencies": {
		"@steeze-ui/heroicons": "^2.2.2",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"@supabase/auth-helpers-sveltekit": "^0.9.4",
		"@supabase/supabase-js": "^2.22.0",
		"class-variance-authority": "^0.6.0",
		"luxon": "^3.3.0"
	}
}
