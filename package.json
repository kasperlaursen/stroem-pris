{
	"name": "stroem-pris",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:fix": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"supabase:types": "npx supabase gen types typescript --project-id 'sdqtbpqrecvpngdqditz' --schema public > types/supabase.ts"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.15.8",
		"@playwright/test": "^1.31.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.8.3",
		"@types/luxon": "^3.2.0",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"@vitest/coverage-c8": "^0.28.5",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.15.8",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"supabase": "^1.38.7",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vitest": "^0.28.5"
	},
	"type": "module",
	"dependencies": {
		"@steeze-ui/heroicons": "^2.2.2",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"@supabase/auth-helpers-sveltekit": "^0.8.7",
		"@supabase/supabase-js": "^2.8.0",
		"luxon": "^3.2.1"
	}
}
