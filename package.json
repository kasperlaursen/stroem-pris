  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "lint:fix": "prettier --plugin-search-dir . --check . && eslint . --fix",
    "format": "prettier --plugin-search-dir . --write .",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "supabase:types": "npx supabase gen types typescript --project-id 'sdqtbpqrecvpngdqditz' --schema public > src/lib/data/supabase/types.ts"
  },
  "devDependencies": {
    "@histoire/plugin-svelte": "^0.16.3",
    "@playwright/test": "^1.36.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.3",
    "@types/luxon": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "histoire": "^0.16.2",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "supabase": "^1.77.9",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@steeze-ui/heroicons": "^2.2.3",
    "@steeze-ui/svelte-icon": "^1.5.0",
    "@supabase/auth-helpers-sveltekit": "^0.10.1",
    "@supabase/supabase-js": "^2.26.0",
    "class-variance-authority": "^0.7.0",
    "eslint-plugin-svelte": "^2.32.4",
    "luxon": "^3.3.0"
