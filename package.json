{
	"name": "stroem-pris",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:fix": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"supabase:types": "npx supabase gen types typescript --project-id 'sdqtbpqrecvpngdqditz' --schema public > src/lib/data/supabase/types.ts"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.15.9",
		"@playwright/test": "^1.32.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.15.1",
		"@types/luxon": "^3.3.0",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitest/coverage-c8": "^0.29.8",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.15.9",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"supabase": "^1.48.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.3",
		"vite": "^4.2.1",
		"vitest": "^0.29.8"
	},
	"type": "module",
	"dependencies": {
		"@steeze-ui/heroicons": "^2.2.2",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"@supabase/auth-helpers-sveltekit": "^0.9.1",
		"@supabase/supabase-js": "^2.14.0",
		"class-variance-authority": "^0.5.3",
		"luxon": "^3.3.0"
	}
}
